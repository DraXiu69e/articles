@<#include <functions/libwiki.c>
@<#include <functions/infobox.c>
@title = homik w denaturacie

%
	infobox (
		infobox_subtitle ("Kurde Niefajna Sprawa");
		infobox_image ("homik_w_denaturacie.jpg", "homik w denaturacie "
		"xddd<br /><span class=\"smaller\">kurde niefajna sprawa</span>");
		infobox_table ( NULL,
			infobox_table_row (
				% Data i miejsce urodzenia %,
				% 21 czerwca 2003, % href("Denaturat")
			);
			infobox_table_row (
				% Imię i nazwisko %,
				% Kurde Niefajna Sprawa %
			);
			infobox_table_row (
				% Pseudonim %,
				% homik w denaturacie xddd, homik %
			);
		);
	);
%

<b>homik w denaturacie xddd</b> (% href("język angielski|ang."); %
<i>hamstr in denaturate xddd</i>; właśc. <b>Kurde Niefajna Sprawa</b>;
ur. % href("2016"); % w % href("Denaturat|Denaturacie"); %) - znany
% href("ornitolog"); %, programista, twórca serwisu % href("Chomikuj.pl"); %.

% h(2, "Życiorys"); %
% h(3, "Wczesne dzieciństwo"); %
<p>
	Urodził się w wieku 23 lat w % href("Denaturat|Denaturacie"); % jako trzecie
	dziecko w małżeństwie % href("Niefajna Sprawa|Niefajnej"); % i %
	href("Kurde Sprawa|Kurdego"); %. Odziedziczył pierwsze % href("imię"); %
	po % href("Ojciec|ojcu"); % oraz drugie imię po % href("Matka|matce"); %.
</p>

<p>
	% href("3"); % lata po narodzinach odkrył w sobie talent % href("Programista|"
	"programisty"); % po czym natychmiastowo przystąpił do tworzenia serwisu
	% href("Chomikuj.pl"); %.
</p>

% h(3, "Późne dzieciństwo"); %
<p>
	Nie ukończył % href("Szkoła Podstawowa|szkoły podstawowej"); %, gdyż jego
	rodzina uważała, że jest zbyt mądry aby się czegoś nauczyć. w wieku 27
	lat usunął z serwera Chomikuj.pl 31 plików % href("DisStream|Norbertowi "
	"Gierczakowi"); %. Spowodowało to pozew w kierunku <b>homika w denaturacie</b>
	który został wystosowany % href("9 września"); % % href("2007"); % przez
	dział prawny Norberta Gierczaka. <b>homik</b> nie pojawił się na %
	href("Sala rozpraw|sali rozpraw"); % w pierwszym terminie, ponieważ -
	jak doniosły media - brał 392-godzinną kąpiel w denaturacie.
	% href("11 listopada"); % tego samego roku pojawił się na %
	href("@#Przebieg rozprawy sądowej|drugiej rozprawie"); % wraz z jego
	prawnikami, m.in. % href("Wielka Chungusica|Wielką Chungusicą"); %.
	Po jego uniewinnieniu kazano usunąć % href("Zanjiawka|zanjiawkę"); %
	Norbertowi Gierczakowi.
</p>

% h(2, "Przebieg rozprawy sądowej"); %
<p>
	<b>homik</b> pojawił się na drugiej rozprawie sądowej w sprawie
	usunięcia 31 plików użytkownika DisStream z serwera Chomikuj.pl.
	Rozprawa ta odbyła się % href("11 listopada"); % % href("2007"); %
	roku o godzinie 16:09 w % href("Centralny Urząd Miejski w Dęblinie|"
	"Centralnym Urzędzie Miejskim w Dęblinie"); %. <b>homik</b> usłyszał
	zarzuty o godzinie 16:20 % href("Czas polski|czasu polskiego"); % po
	czym wypowiedział frazę „<i>% href("JD"); %</i>”. Sędzia który prowadził
	rozprawę, % href("Bagietka Michael"); % dokonał uchylenia wszelakich
	czynów <b>homikowi</b> i nakazał wypłacenie 72 900 zł <b>homikowi</b>.
</p>

